<?xml version="1.0" encoding="UTF-8"?>
<config>
    <!-- CAS验证服务器地址 -->
    <loginServer>http://ca.cupl.edu.cn/tpass</loginServer>
    <validateServer>http://ca.cupl.edu.cn/tpass</validateServer>
    <!--平台地址-->
    <serverName>http://contract.cupl.edu.cn:5051</serverName>
    <!-- 排除被保护的资源  -->
    <notForceAuthUrls>
        <url-pattern>\.js$</url-pattern>
        <url-pattern>\.xml$</url-pattern>
        <url-pattern>\.html.*</url-pattern>
        <url-pattern><![CDATA[.*/fileUpload.*]]></url-pattern>
        <url-pattern><![CDATA[.*login.*]]></url-pattern>
        <url-pattern><![CDATA[.*oauth.*]]></url-pattern>
        <url-pattern><![CDATA[.*findByUsername.*]]></url-pattern>
        <url-pattern><![CDATA[.*insertUsername.*]]></url-pattern>
        <url-pattern><![CDATA[.*remove_token.*]]></url-pattern>
    </notForceAuthUrls>
</config>